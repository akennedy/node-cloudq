doctype
html
  head
    title Cloudq 0.5
    meta(charset='utf8')
    meta(name="viewport", content="width=device-width, initial-scale=1.0")

    link(rel='stylesheet', href='/css/bootstrap.min.css')
    link(rel='stylesheet', href='/css/bootstrap-responsive.min.css')
    link(rel='stylesheet', href='/css/app.css')

  body
    .navbar.navbar-fixed-top
      .navbar-inner
        .container
          a.brand(href="#") Cloudq 0.5
    .container
      = queues.foo.reserved
      .row: .hero-unit: h1 Cloudq Stats
      .row
        table.table.table-bordered
          tr
            th Name
            th Queued
            th Reserved
            th Completed
          each val, key in queues
            tr
              td= key
              td 0
              td= val.reserved
              td= val.completed
